.content{
  padding-top: 100rpx;
}

/* 轮播图 */
.swiper-item {
  width:750rpx;
  height:340rpx;
}

/* 分类 */
.menus{
display: flex;
justify-content: space-between;
align-items: center;
padding: 20rpx;
}
.menus image{
  width:128rpx;
  height:140rpx;
}

/* 楼层 */
.floor{
  margin-top:20rpx;
}
.floor-title image{
  width:100%;
  height:60rpx;
}
.floor-content{
  padding:20rpx;
  display: flex;
  justify-content: space-between;
}
.floor-content-left image{
  width:232rpx;
  height:386rpx;
}
.floor-content-right{
  flex:1;
  margin-left:10rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.floor-content-right image{
  width:233rpx;
  height:188rpx;
}
