/* 历史记录 */
.history{
    display: flex;
    flex-wrap: wrap;
    padding: 0 20rpx; 
    margin-left: -20rpx;
}
.history navigator{
    padding: 0 20rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    border: 1px solid #eeeeee;
    box-sizing: border-box;
    background: #eeeeee;
    margin:20rpx;
    font-size: 14px;
}