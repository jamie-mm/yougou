.seachers{
    display: flex;
    justify-content: space-between;
    padding: 20rpx;
    background: #f6f6f6;
}
.seachers_input{
flex: 1;
display: flex;
align-items: center;
height: 60rpx;
background: #ffffff;
font-size: 13px;
border-radius: 8rpx;
margin-right: 20rpx;
color: #666666;
}
.seachers_input icon{
    margin-right: 10rpx;
}
.quxiao{
    width: 150rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border: 1px solid #eeeeee;
    border-radius: 8rpx;
}
.seacher_content{
    display: flex;
    justify-content: space-between;
    padding: 20rpx;
    color: #666666;
}
